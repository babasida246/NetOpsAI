{
    "name": "@apps/web-cloud",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite dev --port 5173",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "eslint . --ext .ts,.js",
        "typecheck": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --threshold warning",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest run --coverage",
        "test:e2e": "playwright test --config=playwright.config.ts",
        "test:e2e:ui": "playwright test --config=playwright.config.ts --ui",
        "test:e2e:headed": "playwright test --config=playwright.config.ts --headed",
        "test:e2e:report": "playwright show-report",
        "test:all": "pnpm test && pnpm test:e2e"
    },
    "devDependencies": {
        "@babel/parser": "^7.28.6",
        "@babel/traverse": "^7.28.6",
        "@babel/types": "^7.28.6",
        "@playwright/test": "^1.57.0",
        "@sveltejs/adapter-node": "^5.0.0",
        "@sveltejs/kit": "^2.5.27",
        "@sveltejs/vite-plugin-svelte": "^4.0.0",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/svelte": "^5.3.1",
        "@types/node": "^20.0.0",
        "@vitest/ui": "^4.0.16",
        "autoprefixer": "^10.4.0",
        "fast-glob": "^3.3.3",
        "jsdom": "^27.3.0",
        "picocolors": "^1.1.1",
        "postcss": "^8.4.0",
        "svelte": "^5.0.0",
        "svelte-check": "^4.0.0",
        "tailwindcss": "^3.4.0",
        "typescript": "^5.5.0",
        "vite": "^5.4.4",
        "vitest": "^4.0.16"
    },
    "dependencies": {
        "@types/cytoscape": "^3.31.0",
        "cytoscape": "^3.33.1",
        "flowbite-svelte": "^0.46.0",
        "intl-messageformat": "^11.1.1",
        "lucide-svelte": "^0.460.0",
        "svelte-i18n": "^4.0.1"
    }
}