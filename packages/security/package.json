{
    "name": "@security/core",
    "version": "1.0.0",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./netops/*": {
            "import": "./dist/netops/*.js",
            "types": "./dist/netops/*.d.ts"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsup src/index.ts src/netops/redaction.ts --format esm --dts --watch"
    },
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.3.0"
    }
}